{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "es6"
  },
  "jsc": {
    "target": "esnext",
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true,
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "externalHelpers": true
  },
  "sourceMaps": true,
  "minify": false
}
